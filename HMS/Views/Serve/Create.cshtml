@{

	Layout = "~/Views/Shared/_Layout.cshtml";
}
<html>
<head>

	<script type="text/javascript">
		$(document).ready(function () {
			$("#Submit").click(function () {

			
				var VisitorName = $("[name='VisitorName']").val();
				var VisitorAddress = $("[name='VisitorAddress']").val();
				var VisitorPhoneNo = $("[name='VisitorPhoneNo']").val();
				var VisitorEmail = $("[name='VisitorEmail']").val();

				var dataToPost = {
				
					VisitorName: VisitorName,
					VisitorAddress: VisitorAddress,
					VisitorPhoneNo: VisitorPhoneNo,
					VisitorEmail: VisitorEmail

				};
				var c = JSON.stringify(dataToPost);
				alert(c);

				$.ajax(
					{
						type: "Post",
						url: "/api/VisitorsAPI/PostVisitor",
						processData: true,
						dataType: "json",
						contentType: 'application/json',

						data: JSON.stringify(dataToPost),
						success: function () {
							alert('success');
						}


					});
			});
		});
	</script>
	
</head>

	<body>
		<form id="MyForm">
			<div>
			
				Visitor Name : <input name="VisitorName" type="text" /><br />
				Visitor Address : <input name="VisitorAddress" type="text" /><br />
				Visitor PhoneNo:<input name="VisitorPhoneNo" type="text" /><br />
				Visitor Email:<input name="VisitorEmail" type="text" /><br />

				<button type="button" value="Submit" id="Submit"> Save</button>
			</div>
		</form>
	</body>
</html>
